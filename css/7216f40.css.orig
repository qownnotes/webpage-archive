#settings.active #settings-icon {background-position: 0 -43px;}
#settings h3{font-size:13px; line-height:13px; font-family:'Patua One',cursive; color:#000000; padding-top:20px; margin-bottom:12px; text-transform:uppercase;}
#bg-colors li, #color-cheme li{float:left; margin:2px;}
#bg-colors a, #color-cheme a{display:block; border-radius:2px; width:17px; height:17px; padding:2px;}
#bg-colors a.current, #color-cheme a.current{background-color:#535b60 !important;}
#bg-colors a span, #color-cheme  a span{border:1px solid #fff; width:15px; height:15px; display:inline-block;}
#bg-colors{margin-bottom:3px;} 

