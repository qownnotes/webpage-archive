/* portfolio */

#portfolio .item-cont{display:none; padding-bottom:30px;}

#portfolio  #pageination{margin-top:-15px; margin-bottom: 4px;}

.controls{display:inline-block; /*width: 180px;*/ margin: 0 0 25px; vertical-align: top;}

