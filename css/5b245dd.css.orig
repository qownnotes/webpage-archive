.fancybox-fs{ 

    background:url(../bundles/pbebekerleweb/images/close.png) no-repeat 0 0;

    cursor: pointer;

    height: 27px;

    width: 27px;

    left:auto;

    right: 8px;

    top: 8px;

    position: absolute;

    z-index: 1004;}

.fancybox-close{display:none !important;}

	

/* contact */

